{
  "name": "hungknow-webapp",
  "version": "0.1.4",
  "private": true,
  "proxy": "http://localhost:3080",
  "scripts": {
    "eslint:check": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache",
    "eslint:fix": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --fix --cache",
    "dev": "cross-env NODE_ENV='development' node server.js",
    "build": "next build",
    "start": "next start -p 8065",
    "check-types": "tsc",
    "sdk-build": "cd ./node_modules/hkclient-ts && npm ci && npm run prepare",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "run:ci": "npm run check-types && npm run eslint:check && npm run test-ci",
    "test": "cross-env NODE_ENV='development' jest",
    "test-ci": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ=Etc/UTC jest --ci --maxWorkers=8",
    "css-def": "npm-run-all --parallel css-def:component css-def:style css-def:style-2nd",
    "css-def:component": "tsm \"src/components/**/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "css-def:style": "tsm \"src/styles/*/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "css-def:style-2nd": "tsm \"src/styles/*/*/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "git:branch-clean": "git branch -vv | grep ': gone]'|  grep -v \"\\*\" | awk '{ print $1; }' | xargs git branch -D",
    "build:dev:env": "docker-compose -f ./build/docker-compose.dev.yml build",
    "start:dev:env": "docker-compose -f ./build/docker-compose.dev.yml up -d",
    "build:docker-ci": "docker build -f ./build/Dockerfile.ci -t webapp-ci:latest .",
    "task": "node ./tasksfile.js"
  },
  "dependencies": {
    "@formatjs/intl": "1.3.2",
    "@redux-offline/redux-offline": "2.6.0",
    "classnames": "^2.2.6",
    "hkclient-ts": "github:masterhung0112/hkclient-ts#11bf1e557227274278ad4be147192e7abe37ba77",
    "intl-messageformat": "9.3.8",
    "key-mirror": "1.0.1",
    "localforage": "^1.9.0",
    "localforage-observable": "^2.1.1",
    "marked": "^1.1.1",
    "next": "9.5.2",
    "next-redux-wrapper": "^6.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "^5.8.2",
    "react-redux": "^7.2.1",
    "redux-dynamic-modules": "5.2.3",
    "redux-dynamic-modules-thunk": "5.2.3",
    "redux-persist": "4.9.1",
    "redux-saga": "1.1.3",
    "use-async-effect": "2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@blueprintjs/icons": "^3.20.1",
    "@formatjs/intl-pluralrules": "^3.4.7",
    "@formatjs/intl-relativetimeformat": "^7.2.7",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@testing-library/dom": "7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.5",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.6",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.13",
    "@types/marked": "^1.1.0",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-typescript-to-proptypes": "^1.4.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "full-icu": "^1.3.1",
    "http-proxy-middleware": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.21",
    "react-is": "^16.13.1",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "tasksfile": "5.1.1",
    "test-jest": "^1.0.1",
    "ts-jest": "^26.4.0",
    "typed-scss-modules": "^3.1.0",
    "typescript": "^4.0.3"
  }
}
