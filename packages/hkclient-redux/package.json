{
  "name": "hkclient-redux",
  "version": "1.0.1",
  "description": "Client SDK for Hung Know web application",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production ttsc",
    "build:dev": "cross-env NODE_ENV=development ttsc",
    "build:dev:watch": "cross-env NODE_ENV=development ttsc --watch",
    "build:rollup": "cross-env NODE_ENV=production rollup -c",
    "prepare": "npm run build",
    "ci": "npm run check-types && npm run eslint:check && npm run test-ci",
    "eslint:check": "eslint --ignore-path .gitignore --ignore-pattern node_modules --quiet src/**/*.js src/**/*.ts src/**/*.test.ts",
    "eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern node_modules --quiet src/**/*.ts src/**/*.test.ts --fix",
    "dev": "cross-env NODE_ENV=development ttsc --outDir ../hungknow-webapp/node_modules/hkclient-ts/lib",
    "dev:watch": "cross-env NODE_ENV=development ttsc --watch --outDir ../hungknow-webapp/node_modules/hkclient-ts/lib",
    "check-types": "ttsc --noEmit",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=2",
    "test:watch": "cross-env NODE_ENV='development' jest --watch ",
    "test-ci": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ=Etc/UTC jest --ci --maxWorkers=8"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@babel/runtime": "7.12.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/shallow-equals": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "7.18.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-header": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-mattermost": "github:mattermost/eslint-plugin-mattermost#46ad99355644a719bf32082f472048f526605181",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "full-icu": "^1.3.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "mock-socket": "9.0.3",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "rollup": "^2.28.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "26.4.4",
    "tslib": "^2.1.0",
    "ttypescript": "^1.5.12",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.6.0",
    "core-js": "3.8.3",
    "cross-fetch": "^3.0.6",
    "gfycat-sdk": "1.4.18",
    "lodash": "4.17.21",
    "moment-timezone": "^0.5.33",
    "redux": "^4.0.5",
    "redux-action-buffer": "^1.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-dynamic-modules-core": "^5.2.3",
    "redux-persist": "4.9.1",
    "redux-persist-node-storage": "^2.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "1.0.0",
    "rudder-sdk-js": "^1.0.17",
    "serialize-error": "^7.0.1",
    "shallow-equals": "1.0.0"
  },
  "jest-junit": {
    "outputDirectory": "build",
    "outputName": "test-results.xml"
  }
}
