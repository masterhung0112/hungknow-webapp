{
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "build",
    "lib",
  ],
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": true,
    "importHelpers": true,
    "declaration": false,
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": false,
    "noEmit": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "downlevelIteration": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "common": [
        "src/common"
      ],
      "common/*": [
        "src/common/*"
      ],
      "utils": [
        "src/utils"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "i18n/*": [
        "src/i18n/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "actions-types": [
        "src/actions-types"
      ],
      "actions-types/*": [
        "src/actions-types/*"
      ],
      "actions": [
        "src/actions"
      ],
      "actions/*": [
        "src/actions/*"
      ],
      "selectors/*": [
        "src/selectors/*"
      ],
      "reducers/*": [
        "src/reducers/*"
      ],
      "mocks": [
        "src/mocks"
      ],
      "mocks/*": [
        "src/mocks/*"
      ],
      "showroom": [
        "src/core/showroom"
      ],
      "showroom/*": [
        "src/core/showroom/*"
      ],
      "tests": [
        "src/tests"
      ],
      "tests/*": [
        "src/tests/*"
      ],
      "types/*": [
        "src/types/*"
      ],
      "store": [
        "src/store"
      ],
      "store/*": [
        "src/store/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
      "constants": [
        "src/constants"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "storybook/*": [
        "src/storybook/*"
      ],
      "client/*": [
        "src/client/*"
      ],
      "hkclient-redux/*": [
        "./node_modules/hkclient-redux/dist/*"
      ],
      "modules/*": [
        "src/modules/*"
      ],
      "plugins/*": [
        "src/plugins/*"
      ],
      "core": [
        "src/core"
      ],
      "core/*": [
        "src/core/*"
      ]
    },
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  }
}
