{
  "name": "hk-browser",
  "version": "0.1.4",
  "private": true,
  "proxy": "http://localhost:3080",
  "scripts": {
    "eslint:check": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache",
    "eslint:fix": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --fix --cache",
    "dev": "cross-env NODE_ENV='development' node server.js",
    "next:build": "next build",
    "next:start": "next start",
    "build": "shx rm -rf dist && cross-env NODE_ENV=production webpack",
    "build:dev": "shx rm -rf dist && cross-env NODE_ENV=development webpack",
    "run": "webpack --progress --watch",
    "dev-server": "webpack serve --mode development",
    "check-types": "tsc",
    "sdk-build": "cd ./node_modules/hkclient-ts && npm ci && npm run prepare",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "ci": "npm run check-types && npm run eslint:check && npm run test-ci",
    "updatesnapshot": "jest --updateSnapshot",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu NODE_OPTIONS='--trace-warnings' TZ=Etc/UTC jest",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ=Etc/UTC jest --watch",
    "test-ci": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ=Etc/UTC jest --ci --maxWorkers=8",
    "css-def": "npm-run-all --parallel css-def:component css-def:style css-def:style-2nd",
    "css-def:component": "tsm \"src/components/**/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "css-def:style": "tsm \"src/styles/*/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "css-def:style-2nd": "tsm \"src/styles/*/*/*.scss\" --nameFormat none --aliases.~@blueprintjs/icons/src/icons \"./node_modules/@blueprintjs/icons/src/icons\" --exportType default",
    "git:branch-clean": "git branch -vv | grep ': gone]'|  grep -v \"\\*\" | awk '{ print $1; }' | xargs git branch -D",
    "build:dev:env": "docker-compose -f ./build/docker-compose.dev.yml build",
    "start:dev:env": "docker-compose -f ./build/docker-compose.dev.yml up -d",
    "build:docker-ci": "docker build -f ./build/Dockerfile.ci -t webapp-ci:latest .",
    "task": "node ./tasksfile.js"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "5.3.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "5.2.0",
    "redux-thunk": "2.3.0",
    "semver": "7.3.5",
    "reselect": "1.0.0",
    "hkclient-redux": "1.0.1",
    "hk-chat": "1.0.0",
    "katex": "0.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@blueprintjs/icons": "^3.25.1",
    "@hot-loader/react-dom": "^17.0.2",
    "@types/classnames": "^2.2.10",
    "@types/history": "4.7.8",
    "@types/jest": "^26.0.13",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.35",
    "@types/react": "^17.0.38",
    "@types/react-redux": "^7.1.9",
    "@types/react-select": "3.0.19",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "autoprefixer": "^10.2.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-typescript-to-proptypes": "^1.4.1",
    "copy-webpack-plugin": "8.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.1.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "7.24.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jquery": "1.5.1",
    "eslint-plugin-mattermost": "github:mattermost/eslint-plugin-mattermost#46ad99355644a719bf32082f472048f526605181",
    "eslint-plugin-no-only-tests": "2.5.0",
    "eslint-plugin-react": "7.23.2",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "full-icu": "^1.3.1",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "http-proxy-middleware": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "7.0.1",
    "isomorphic-fetch": "3.0.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "2.3.1",
    "mini-css-extract-plugin": "1.4.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.2.0",
    "prettier": "2.3.2",
    "react-hot-loader": "^4.13.0",
    "react-router-enzyme-context": "1.2.0",
    "sass": "1.32.8",
    "sass-loader": "^10.1.1",
    "style-loader": "2.0.0",
    "set-tz": "0.2.0",
    "source-map-loader": "2.0.1",
    "tasksfile": "5.1.1",
    "test-jest": "^1.0.1",
    "ts-jest": "26.5.3",
    "typed-scss-modules": "^4.1.1",
    "typescript": "4.2.4",
    "webpack": "5.32.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-livereload-plugin": "3.0.1",
    "webpack-node-externals": "2.5.2",
    "webpack-pwa-manifest": "4.3.0",
    "babel-plugin-module-resolver": "4.1.0",
    "compass-mixins": "0.12.10",
    "shx": "^0.3.3"
  }
}
